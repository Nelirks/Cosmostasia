; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cosmostasia"
run/main_scene="res://scenes/game_scenes/main_menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

WwiseRuntimeManager="*res://addons/Wwise/runtime/wwise_runtime_manager.gd"
GameManager="*res://scenes/game_management/game_manager.tscn"
NetworkManager="*res://scenes/network/network_manager.tscn"
DescriptionHandler="*res://scenes/info_popup/description_handler.tscn"
AudioManager="*res://scenes/audio/audio_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/Wwise/plugin.cfg")

[wwise]

common_user_settings/startup_language="English(US)"
